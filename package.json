{
  "name": "fullstack-web-study",
  "author": "Flock.Community",
  "version": "1.0.0",
  "description": "Full stack web application architectures, a comparative study",
  "keywords": [
    "htmx",
    "koajs",
    "nextjs",
    "full-stack",
    "web"
  ],
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "run-p dev:*",
    "dev:db": "npm run dev --workspace=js-app-db",
    "dev:koa": "npm run dev --workspace=koa-htmx",
    "dev:next": "npm run dev --workspace=nextjs-app",
    "start": "npm start --workspaces",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flock-community/fullstack-web-study.git"
  },
  "bugs": {
    "url": "https://github.com/flock-community/fullstack-web-study/issues"
  },
  "homepage": "https://github.com/flock-community/fullstack-web-study#readme"
}
